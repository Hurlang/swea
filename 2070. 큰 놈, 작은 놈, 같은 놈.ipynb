    "T=int(input())\n",
    "for test_case in range(1,T+1):\n",
    "    A,B=map(int,(input().split()))\n",
    "    symbol=''\n",
    "    if A>B:\n",
    "        symbol='>'\n",
    "    elif A==B:\n",
    "        symbol='='\n",
    "    else:\n",
    "        symbol='<'\n",
    "    print(f'#{test_case} {symbol}')"
   ]
